import printf;

int n;
int a[4];

void insertionSort() {
  int i;
  for (i = 1; i < n; i += 1) {
    int key;
    int j;
    key = a[i];
    j = i - 1;
    while (j >= 0 && a[j] > key) {
      a[j + 1] = a[j];
      j -= 1;
    }
    a[j + 1] = key;
  }
}

void main() {
  int i;

  a[0] = 7;
  a[1] = 4;
  a[2] = 6;
  a[3] = 2;
  n = 4;

  insertionSort();    // Causing infinite loop right now. fix later

  printf("%d\n", a[0]);
  printf("%d\n", a[1]);
  printf("%d\n", a[2]);
  printf("%d\n", a[3]);
}
