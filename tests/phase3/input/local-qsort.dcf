import printf;

int a[5];

int partition(int low, int high) {
  int pivot;
  int i;
  int j;
  int temp2;

  pivot = a[high];
  i = low - 1;

  for (j = low; j < high; j += 1) {
    if (a[j] < pivot) {
      int temp;
      i += 1;
      temp = a[i];
      a[i] = a[j];
      a[j] = temp;
    }
  }

  temp2 = a[i + 1];
  a[i + 1] = a[high];
  a[high] = temp2;
  return i + 1;
}

void quicksort(int low, int high) {
  if (low < high) {
    int pi;
    pi = partition(low, high);
    quicksort(low, pi - 1);
    quicksort(pi + 1, high);
  }
}

void main() {
  a[0] = 4;
  a[1] = 2;
  a[2] = 5;
  a[3] = 1;
  a[4] = 3;

  quicksort(0, 4);

  printf(a[0]);
  printf(a[1]);
  printf(a[2]);
  printf(a[3]);
  printf(a[4]);
}
